      "source": [
        "import cv2\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.signal import convolve2d\n",
        "from sklearn.cluster import KMeans\n",
        "\n",
        "# Función para detección de bordes Canny\n",
        "\n",
        "def apply_gaussian_filter(image, sigma=1.0):\n",
        "    kernel_size = int(6 * sigma) + 1\n",
        "    kernel = np.fromfunction(\n",
        "        lambda x, y: (1/ (2 * np.pi * sigma ** 2)) * np.exp(-((x - (kernel_size - 1) / 2) ** 2 + (y - (kernel_size - 1) / 2) ** 2) / (2 * sigma ** 2)),\n",
        "        (kernel_size, kernel_size)\n",
        "    )\n",
        "    kernel = kernel / np.sum(kernel)\n",
        "    smoothed_image = convolve2d(image, kernel, mode='same', boundary='wrap')\n",
        "    return smoothed_image\n",
        "\n",
        "def calculate_custom_gradients(image):\n",
        "    gradient_x = np.array([[-1, 0, 1], [-2, 0, 2], [-1, 0, 1]])\n",
        "    gradient_y = np.array([[-1, -2, -1], [0, 0, 0], [1, 2, 1]])\n",
        "    gradient_x_image = convolve2d(image, gradient_x, mode='same', boundary='wrap')\n",
        "    gradient_y_image = convolve2d(image, gradient_y, mode='same', boundary='wrap')\n",
        "\n",
        "    gradient_magnitude = np.sqrt(gradient_x_image ** 2 + gradient_y_image ** 2)\n",
        "    hist, bins = np.histogram(gradient_magnitude, bins=100)\n",
        "\n",
        "    return gradient_x_image, gradient_y_image\n",
        "\n",
        "# Función para calcular la dirección del gradiente\n",
        "def calculate_gradient_direction(gradient_x_image, gradient_y_image):\n",
        "    gradient_direction = np.arctan2(gradient_y_image, gradient_x_image)\n",
        "    return gradient_direction\n",
        "\n",
        "# Función para aplicar la supresión de no máximos\n",
        "def apply_non_maximum_suppression(gradient_magnitude, gradient_direction):\n",
        "    height, width = gradient_magnitude.shape\n",
        "    non_maximum_suppressed = np.zeros_like(gradient_magnitude)\n",
        "    for i in range(1, height - 1):\n",
        "        for j in range(1, width - 1):\n",
        "            angle = gradient_direction[i, j]\n",
        "            if (angle >= -np.pi/8 and angle <= np.pi/8) or (angle <= -7*np.pi/8 or angle >= 7*np.pi/8):\n",
        "                if gradient_magnitude[i, j] >= gradient_magnitude[i, j + 1] and gradient_magnitude[i, j] >= gradient_magnitude[i, j - 1]:\n",
        "                    non_maximum_suppressed[i, j] = gradient_magnitude[i, j]\n",
        "            elif (angle >= np.pi/8 and angle <= 3*np.pi/8) or (angle <= -5*np.pi/8 or angle >= -7*np.pi/8):\n",
        "                if gradient_magnitude[i, j] >= gradient_magnitude[i - 1, j + 1] and gradient_magnitude[i, j] >= gradient_magnitude[i + 1, j - 1]:\n",
        "                    non_maximum_suppressed[i, j] = gradient_magnitude[i, j]\n",
        "            elif (angle >= 3*np.pi/8 and angle <= 5*np.pi/8) or (angle <= -3*np.pi/8 or angle >= -5*np.pi/8):\n",
        "                if gradient_magnitude[i, j] >= gradient_magnitude[i - 1, j] and gradient_magnitude[i, j] >= gradient_magnitude[i + 1, j]:\n",
        "                    non_maximum_suppressed[i, j] = gradient_magnitude[i, j]\n",
        "            elif (angle >= 5*np.pi/8 and angle <= 7*np.pi/8) or (angle <= -np.pi/8 and angle >= -3*np.pi/8):\n",
        "                if gradient_magnitude[i, j] >= gradient_magnitude[i - 1, j - 1] and gradient_magnitude[i, j] >= gradient_magnitude[i + 1, j + 1]:\n",
        "                    non_maximum_suppressed[i, j] = gradient_magnitude[i, j]\n",
        "    return non_maximum_suppressed\n",
        "\n",
        "def hysteresis_threshold(canny_edges, low_threshold, high_threshold):\n",
        "    height, width = canny_edges.shape\n",
        "    result = np.zeros((height, width), dtype=np.uint8)\n",
        "    strong_edge_value = 255\n",
        "\n",
        "    strong_i, strong_j = np.where(canny_edges >= high_threshold)\n",
        "    result[strong_i, strong_j] = strong_edge_value\n",
        "\n",
        "    # Crear una máscara de bordes débiles dentro del rango de umbrales\n",
        "    weak_edge_mask = (canny_edges >= low_threshold) & (canny_edges < high_threshold)\n",
        "\n",
        "    # Definir vecinos para exploración\n",
        "    neighbors_i = np.array([-1, -1, -1, 0, 0, 1, 1, 1])\n",
        "    neighbors_j = np.array([-1, 0, 1, -1, 1, -1, 0, 1])\n",
        "\n",
        "    for i in range(1, height - 1):\n",
        "        for j in range(1, width - 1):\n",
        "            if weak_edge_mask[i, j]:\n",
        "                # Comprobar si al menos un vecino es un borde fuerte\n",
        "                neighbor_values = canny_edges[i + neighbors_i, j + neighbors_j]\n",
        "                if np.any(neighbor_values >= high_threshold):\n",
        "                    result[i, j] = strong_edge_value\n",
        "\n",
        "    return result\n",
        "\n",
        "# Definir la función small_objects\n",
        "def small_objects(imagen_binaria, min_area):\n",
        "    # Encontrar contornos en la imagen binaria\n",
        "    contours, _ = cv2.findContours(imagen_binaria, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
        "\n",
        "    # Iterar sobre cada contorno\n",
        "    for contour in contours:\n",
        "        area = cv2.contourArea(contour)\n",
        "        if area < min_area:\n",
        "            # Rellenar el contorno con cero (eliminar objeto)\n",
        "            cv2.drawContours(imagen_binaria, [contour], -1, 0, thickness=cv2.FILLED)\n",
        "\n",
        "    return imagen_binaria\n",
        "\n",
        "# Funciones para K-Means\n",
        "def distancia(punto1, punto2):\n",
        "    return np.sqrt(np.sum((punto1 - punto2) ** 2))\n",
        "\n",
        "def asignar_puntos_a_centroides(datos, centroides):\n",
        "    asignaciones = []\n",
        "    for punto in datos:\n",
        "        distancias = [distancia(punto, centroide) for centroide in centroides]\n",
        "        asignaciones.append(np.argmin(distancias))\n",
        "    return np.array(asignaciones)\n",
        "\n",
        "def calcular_nuevos_centroides(datos, asignaciones, K):\n",
        "    nuevos_centroides = []\n",
        "    for i in range(K):\n",
        "        puntos_asignados = datos[asignaciones == i]\n",
        "        if len(puntos_asignados) > 0:\n",
        "            nuevo_centroide = np.mean(puntos_asignados, axis=0)\n",
        "        else:\n",
        "            # Si no hay puntos asignados a este centroide, mantener el centroide actual\n",
        "            nuevo_centroide = centroides[i]\n",
        "        nuevos_centroides.append(nuevo_centroide)\n",
        "    return np.array(nuevos_centroides)\n",
        "\n",
        "# Cargar la imagen\n",
        "image_path = \"bola.png\"  # Reemplaza con la ruta de tu imagen\n",
        "image = cv2.imread(image_path, 1)\n",
        "input_image = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n",
        "\n",
        "\n",
        "# Parámetros de K-Means\n",
        "K = 7  # Número de clusters deseado\n",
        "tolerancia = 1e-4\n",
        "\n",
        "# Inicialización de centroides al azar\n",
        "np.random.seed(0)\n",
        "X = np.array(image.reshape(-1, 3), dtype=np.float32)\n",
        "centroides = X[np.random.choice(len(X), K, replace=False)]\n",
        "\n",
        "# Iterar hasta que los centroides converjan\n",
        "for _ in range(100):  # Límite máximo de iteraciones\n",
        "    asignaciones = asignar_puntos_a_centroides(X, centroides)\n",
        "    nuevos_centroides = np.array([X[asignaciones == i].mean(axis=0) if np.sum(asignaciones == i) > 0 else centroides[i] for i in range(K)])\n",
        "\n",
        "    # Comprobar la convergencia\n",
        "    if np.all(np.abs(centroides - nuevos_centroides) < tolerancia):\n",
        "        break\n",
        "    centroides = nuevos_centroides\n",
        "\n",
        "\n",
        "# Convertir la imagen a escala de grises\n",
        "grayscale_image = cv2.imread(image_path, 0).astype(float)\n",
        "\n",
        "# Aplicar el filtro gaussiano\n",
        "smoothed_image = apply_gaussian_filter(grayscale_image)\n",
        "\n",
        "# Calcular los gradientes personalizados\n",
        "gradient_x_image, gradient_y_image = calculate_custom_gradients(smoothed_image)\n",
        "\n",
        "# Calcular la dirección del gradiente\n",
        "gradient_direction = calculate_gradient_direction(gradient_x_image, gradient_y_image)\n",
        "\n",
        "# Aplicar la supresión de no máximos\n",
        "gradient_magnitude = np.sqrt(gradient_x_image ** 2 + gradient_y_image ** 2)\n",
        "non_maximum_suppressed = apply_non_maximum_suppression(gradient_magnitude, gradient_direction)\n",
        "\n",
        "# Segmentar la imagen utilizando las asignaciones de K-Means\n",
        "segmented_image = centroides[asignaciones].reshape(image.shape).astype(np.uint8)\n",
        "\n",
        "threshold1 = 10 # Umbral inferior\n",
        "threshold2 = 40  # Umbral superior\n",
        "\n",
        "# Aplicar detección de bordes Canny\n",
        "canny_edges = hysteresis_threshold(non_maximum_suppressed, threshold1, threshold2)\n",
        "\n",
        "#A PARTIR DE AQUÍ MÉTODO DEL CODO\n",
        "\n",
        "# Rango de valores de K para probar\n",
        "k_values = range(1, 11)  # Prueba desde 1 hasta 10 clústeres\n",
        "\n",
        "# Lista para almacenar las inercias\n",
        "inertia_values = []\n",
        "\n",
        "# Iterar a través de diferentes valores de K y calcular la inercia\n",
        "for k in k_values:\n",
        "    # Ejecutar K-Means para el valor actual de K\n",
        "    kmeans = KMeans(n_clusters=k,  n_init=10, random_state=0).fit(X)\n",
        "    # Obtener la inercia del modelo K-Means\n",
        "    inertia = kmeans.inertia_\n",
        "    inertia_values.append(inertia)\n",
        "\n",
        "# Graficar la curva de la inercia en función de K\n",
        "plt.plot(k_values, inertia_values, marker='o')\n",
        "plt.xlabel('Número de Clústeres (K)')\n",
        "plt.ylabel('Inercia')\n",
        "plt.title('Método del Codo para Determinar K')\n",
        "plt.grid(True)\n",
        "plt.show()"
      ]
    }
